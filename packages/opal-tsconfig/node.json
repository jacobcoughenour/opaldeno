{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "node",
	"extends": "./base.json",
	"compilerOptions": {
		"lib": ["ES2020", "DOM"],
		"module": "commonjs",
		"target": "es2020",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}
